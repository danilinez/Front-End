              | Lorem ipsum dolor sit amet, per no tamquam omittam appellantur.
              | Meis fierent id est, hinc reque intellegat.
            ul.list-inline.app-btns
              li.list-inline-item
                a.mr-2(href='asd')
                  img(src='assets/images/astore.png', alt='app_store')
              li.list-inline-item
                a(href='asd')
                  img(src='assets/images/gplay.png', alt='google_play')
        img.banner_bg(src='assets/images/carrier/banner.png', alt='banner')

      //- Banner End 
      
      //- Why ICarryIt
      .why_icarry
        .scroll-for-more
          a(href='javascript:void(0)')
            img.arrow-down(src='assets/images/down-arrow.svg', alt='chevron_down')
            | Scroll for More
        .container
          h1.common-heading Why Carry With ICarryIt?            
          .phone-holder
            img.leftcurve(src='assets/images/left_curve.png', alt='mobile')
            img.rightcurve(src='assets/images/right_curve.png', alt='mobile')
            ul.left-list
              li
                span Choose the Cargo that Suits You
                img(src='assets/images/package.png', alt='mobile')
              li.mid_item
                span In-App Pickup & Delivery Verification
                img(src='assets/images/check.png', alt='mobile')
              li
                span Easily Track Live & Completed Orders
                img(src='assets/images/todos.png', alt='mobile')
            img.img-fluid.phone(src='assets/images/carrier/phone.png', alt='mobile')
            ul.right-list
              li
                img(src='assets/images/lock.png', alt='mobile')
                span Hassle Free Payments
              li.mid_item
                img(src='assets/images/map.png', alt='mobile')
                span Optimised Delivery Routes
              li
                img(src='assets/images/conversation.png', alt='mobile')
                span Contact the Client at Any Time
          
      
