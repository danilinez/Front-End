      //- Video Banner
      .video
        .container-fluid
          .video_wrapper
            .play_video
              a(data-toggle='modal', href='#video_popup')
                img.img-fluid(src='assets/images/carrier/play.png', alt='video_play')
            img.img-fluid(src='assets/images/video_banner.jpg', alt='video_banner')
    
            #video_popup.modal.fade
              .modal-dialog.modal-lg
                .modal-content
                  .modal-body
                    button.close(type='button', data-dismiss='modal') Ã—
                    video.carry_video(width='100%', height='auto')
                      source(src='assets/video/icarry.mp4', type='video/mp4')
