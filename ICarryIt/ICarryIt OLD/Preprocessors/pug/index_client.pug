            img.img-fluid.phone(src='assets/images/client/phone.png', alt='mobile')
            ul.right-list
              li
                img(src='assets/images/lock.png', alt='mobile')
                span Easy and Secure In-App Payment
              li.mid_item
                img(src='assets/images/deliver.png', alt='mobile')
                span Same Day Tracked Delivery
              li
                img(src='assets/images/conversation.png', alt='mobile')
                span Contact the Carrier at Any Time

